{
    "expo": {
        "name": "Wayzza",
        "slug": "where-i-was",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icons/icon.png",
        "scheme": "whereiwas",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "icon": "./assets/icons/icon.png",
            "supportsTablet": true,
            "infoPlist": {
                "NSLocationWhenInUseUsageDescription": "This app needs access to location when open to track your trips.",
                "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to track your trips even when running in the background.",
                "NSLocationAlwaysUsageDescription": "This app needs access to location to track your trips even when running in the background.",
                "UIBackgroundModes": ["location"]
            },
            "bundleIdentifier": "com.ctsjwtest.whereiwas"
        },
        "android": {
            "icon": "./assets/icons/icon.png",
            "adaptiveIcon": {
                "foregroundImage": "./assets/icons/android_adaptive_foreground_1080.png",
                "backgroundImage": "./assets/icons/android_adaptive_background_1080.png"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "permissions": [
                "ACCESS_FINE_LOCATION",
                "ACCESS_COARSE_LOCATION",
                "ACCESS_BACKGROUND_LOCATION",
                "FOREGROUND_SERVICE",
                "FOREGROUND_SERVICE_LOCATION"
            ],
            "package": "com.ctsjwtest.whereiwas"
        },
        "web": {
            "output": "static",
            "favicon": "./assets/icons/icon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#f5f5f5",
                    "dark": {
                        "backgroundColor": "#111827"
                    }
                }
            ],
            "expo-sqlite",
            "expo-web-browser",
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to track your trips.",
                    "isAndroidBackgroundLocationEnabled": true,
                    "isAndroidForegroundServiceEnabled": true
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "55b91b8a-2985-4059-b08a-b924879f24ac"
            }
        }
    }
}
